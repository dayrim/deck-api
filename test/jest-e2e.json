{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "rootDir": "../",
  "moduleNameMapper": {
    "src/(.*)": "<rootDir>/src/$1"
  },
  "moduleDirectories": [
    "node_modules",
    "src"
  ],
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  }
}